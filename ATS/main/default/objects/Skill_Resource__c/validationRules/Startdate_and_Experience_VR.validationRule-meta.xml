<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Startdate_and_Experience_VR</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK( Start_date__c ), ISBLANK( Experience__c ))</errorConditionFormula>
    <errorMessage>Either Start Date or Experience need to be filled</errorMessage>
</ValidationRule>
