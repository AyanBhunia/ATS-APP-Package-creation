<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Submitted_Date</fullName>
    <active>false</active>
    <errorConditionFormula>OR(Submitted_Date__c  &lt;  Internal_Verification_Date__c, Submitted_Date__c  &gt;  TODAY())</errorConditionFormula>
    <errorDisplayField>Submitted_Date__c</errorDisplayField>
    <errorMessage>Submitted date should be between Internal verification date and Today date</errorMessage>
</ValidationRule>
